{
    "name": "jeckel/doctrine-extend-bundle",
    "description": "Extend some doctrine's functionalities with symfony and jeckel-lab's contract",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.2 || ^8.0",
        "jeckel-lab/clock": "^1.0",
        "doctrine/orm": "^2.8",
        "doctrine/event-manager": "^1.1",
        "symfony/string": "^5.2",
        "symfony/http-kernel": "^5.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "vimeo/psalm": "^4.3",
        "phpmd/phpmd": "^2.9",
        "squizlabs/php_codesniffer": "^3.5",
        "phpro/grumphp": "^0.19 || ^1.2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Mercier-Rojas",
            "email": "julien@jeckel-lab.fr"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "JeckelLab\\DoctrineExtendBundle\\": "src/"
        }
    }
}
